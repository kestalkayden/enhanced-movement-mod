{
  "schemaVersion": 1,
  "id": "enhancedmovement",
  "version": "${version}",

  "name": "Enhanced Movement",
  "description": "A mod that enables double jump, dash in any direction, and ledge grabbing! Enhanced with modern config GUI.",
  "authors": [
    "Kestalkayden"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/kestalkayden/enhanced-movement-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/enhancedmovement/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.fabricmc.EnhancedMovement.EnhancedMovement"
    ],
    "client": [
      "net.fabricmc.EnhancedMovement.client.EnhancedMovementClient"
    ],
    "modmenu": [
      "net.fabricmc.EnhancedMovement.client.ModMenuIntegration"
    ]
  },
  "mixins": [
    "enhancedmovement.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.14",
    "minecraft": "~1.21.8",
    "java": ">=21",
    "fabric-api": "*"
  },
  "suggests": {
    "modmenu": "*"
  }
}
